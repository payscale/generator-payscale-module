{
  "name": "<%- name %>",
  "version": "0.0.1",
  "description": "<%- description %>",
  "main": "lib/index.js",
  "scripts": {
    "test": "<%- test %>",
    "lint": "eslint --config .eslintrc --format table src",
    "lint-fix": "eslint --fix --config .eslintrc --format table src",
    "teamcity-test": "<%- teamcitytest %>",
    "teamcity-lint": "eslint --config .eslintrc --format ./node_modules/eslint-teamcity/index.js src",
    "compile": "gulp && webpack",
    "precommit": "lint-staged",
    "prepublish": "npm run teamcity-lint && npm run teamcity-test && npm run compile",
    "start": "start-storybook -p 9001 -c .storybook"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write \"**/*.js\"",
      "eslint --fix \"src/**/*.js\"",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "<%- repo %>"
  },
  "keywords": [],
  "author": "<%- author %>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@storybook/addon-knobs": "3.4.8",
    "@storybook/react": "3.4.8",
    "babel-cli": "6.24.0",
    "babel-core": "6.26.3",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-stage-1": "6.16.0",
    "babel-register": "6.18.0",
    "babelify": "7.3.0",
    "eslint": "3.15.0",
    "eslint-config-payscale": "1.0.3",
    "eslint-plugin-react": "6.9.0",
    "eslint-teamcity": "1.3.1",
    "extract-text-webpack-plugin": "2.1.0",
    "husky": "0.14.3",
    "jsdom": "9.9.1",
    "lint-staged": "7.2.0",
    "prettier": "1.12.1",
    "webpack": "2.2.0"
  },
  "dependencies": {}
}
